package ch08;

public class Product {
	private String id;
	private String name;
	private String co;
	private String price;
	private String date;
	
	// 생성자 선언
	public Product (String id, String name, String co, String price, String date) {
		this.id=id;
		this.name=name;
		this.co=co;
		this.price=price;
		this.date=date;
	}

	public String getId() {
		return id;
	}

	public String getName() {
		return name;
	}

	public String getCo() {
		return co;
	}


	public String getPrice() {
		return price;
	}


	public String getDate() {
		return date;
	}
	
}
